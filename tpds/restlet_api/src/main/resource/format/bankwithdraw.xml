<?xml version="1.0" encoding="UTF-8"?>
<root>
<reqHeader obj="json">
	<version type="varchar" value="3" code="1002" req="M">version字段不应超过3位有效字符</version>
	<merchantCode type="varchar" value="11" code="1002" req="M">merchantCode字段应为11位有效字符</merchantCode>
	<txnType type="varchar" value="6" code="1002" req="M">txnType字段不应超过6位有效字符</txnType>
	<clientSn type="varchar" value="50" code="1002" req="M">clientSn字段不应超过50位有效字段</clientSn>
	<clientDate type="char" value="8" code="1002" req="M">clientDate字段的日期格式为YYYYMMDD</clientDate>
	<clientTime type="char" value="6" code="1002" req="M">clientTime字段的日期格式为HHmmss</clientTime>
	<fileName type="varchar" value="255" code="1002" req="O">fileName字段不应超过255位有效字符</fileName>
	<signTime type="varchar" value="30" code="1002" req="M">signTime字段不应超过30位有效字符</signTime>
	<signature type="varchar" value="4000" code="1002" req="M">signature字段不应超过4000位有效字符</signature>
</reqHeader>
<inBody obj="json">
	<businessSeqNo type="varchar" value="26" req="M">businessSeqNo字段不应超过26位有效字符</businessSeqNo>
	<rType type="varchar" value="3" req="M">rType字段应为3位有效字符</rType>
	<currency type="varchar" value="3" req="M">currency字段超过了3位有效字段</currency>
	<amount type="double" req="M">amount不是数字类型</amount>
	<bankAccountNo type="varchar" value="22" req="M">bankAccountNo字段超过了22位有效字段</bankAccountNo>
	<bankBranch type="varchar" value="12" req="M">bankBranch字段超过了12位有效字段</bankBranch>
	<certNo type="varchar" value="30" req="M">certNo字段超过了30位有效字段</certNo>
	<note type="varchar" value="100" req="O">note字段应为100位有效字符</note>
</inBody>
</root>